<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>project overview</title>
    <link rel="stylesheet" href="/css/main.css" />
  </head>
  <body>
    <div class="container">
      <% projects.map(data=> { %>
      <div class="text-center">
        <h2><%= data.name %></h2>
        <p>
          <i class="fa-solid fa-pen"></i>
          Created by: <b><%= data.user %></b>
        </p>
      </div>
      <div class="row">
        <div class="col-md-8"></div>
        <div class="col-md-4 d-grid gap-2">
          <a
            href="/project/projectSettings/<%= data.id %>"
            class="btn btn-primary"
            ><i class="fa-solid fa-gear me-2"></i>Edit project</a
          >
          <a href="#members" class="btn btn-secondary"><i class="fa-solid fa-users me-2"></i>Members</a>
          <a href="#topics" class="btn btn-secondary"><i class="fa-solid fa-comment me-2"></i>Topics</a>
          <a href="#taks" class="btn btn-secondary"><i class="fa-solid fa-list me-2"></i>Taks</a>
          <a href="#attachments" class="btn btn-secondary"><i class="fa-solid fa-file me-2"></i>Attachments</a>
        </div>
      </div>
      <% })%>
    </div>
  </body>
</html>
